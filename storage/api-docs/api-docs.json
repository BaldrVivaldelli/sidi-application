{
    "openapi": "3.0.0",
    "info": {
        "title": "API PUBLICA",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://swagger.local"
        }
    ],
    "paths": {
        "/api/getAllData": {
            "get": {
                "summary": "Mostrar lista de contenido",
                "operationId": "App\\Http\\Controllers\\PublicController::getAllData",
                "responses": {
                    "200": {
                        "description": "Mostrar todos el contenido."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/getById/id": {
            "get": {
                "summary": "obtiene el contenido",
                "operationId": "App\\Http\\Controllers\\PublicController::getById",
                "responses": {
                    "200": {
                        "description": "Devuelve el contenido."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        }
    }
}